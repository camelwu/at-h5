    <script>
        (function(){
            //阻止在嵌套元素中传播
            function cancelPropagation (event){
                event = window.event||event;
                if( document.all){
                    event.cancelBubble = true;
                }else{
                    event.stopPropagation();
                }
            }
            $("#state_btn").click(function(){
                $(".js-mask").show();
                $('#traState').show();
                $('#traState').css({"height":"50%"});
                $('#sendState').hide();
                $('.sc-content').css({"overflow":"hidden"})
            });
            $(".js-travelnotes-close").click(function(){
                $(".js-mask").hide();
                $('.sc-content').css({"overflow":"auto"})
            });
            $(".js-mask").click(function(){
                $(".js-mask").hide();
                $('.sc-content').css({"overflow":"auto"})
            });
            $(".js-travelnotes").click(function(e){
                cancelPropagation(e);
            });
            $("#send_btn").click(function(){
                $(".js-mask").show();
                $('#traState').hide();
                $('#sendState').show();
//                $('#sendState').css({"height":"35%"});
                $('.sc-content').css({"overflow":"hidden"});
            });
        })();
    </script>
