/*城市列表的css*/
.cl_box_box{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    height: 100%;
    width: 100%;
    overflow-y: auto;
    z-index: 138;
    display: none;
    -webkit-overflow-scrolling: touch;
}

.hotel-header{
    position: fixed;
    top: 0;
    left: 0;
    pointer-events: all;
    z-index: 99;
    background: -webkit-linear-gradient(#16364f,#194361);
    background: -o-linear-gradient(#16364f,#194361);
    background: -moz-linear-gradient(#16364f,#194361);
    background: linear-gradient(#16364f,#194361);
    width: 100%;
    min-height: 46px;
    max-height: 100%;
    transform: translateY(0px);
    -webkit-transform: translateY(0px);
    transition: all 250ms ease-in-out;
    border-bottom: solid 1px rgba(0,0,0,0.1);
}

.cl_box{
    position:absolute;left:0;right:0;top:0;bottom:0;overflow:auto;
    -webkit-overflow-scrolling: touch;
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
}
