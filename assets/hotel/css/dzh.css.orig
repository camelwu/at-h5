    padding:0 .4rem ;
}
.hotel_destination_left{
    display: inline-block;
    width: 28%;
}
.hotel_destination_right{
    display: inline-block;
  width: 65%;
}
.hotel_destination_right span{
  display: block;
  width: 100%;
}
.hotel_destination_right i{
  position: absolute;
  right: .4rem;
  top: .38rem;
  width: .4rem;
  height: .5rem;
